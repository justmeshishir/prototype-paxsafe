<h1>Hello Paxsafers</h1>
<% @apartments.each do |apartment| %>
	<%= apartment.name %><br />
	<%= apartment.rate %><br />
	<%= apartment.address %><br />
	<%= apartment.description %><br /><br /><br />
<% end %>
<%= simple_form_for Apartment.new do |f| %>
  <%= f.input :name %>
  <%= f.input :rate %>
  <%= f.input :address, id: 'address'%>
  <%= f.input :description  %>
  <%= f.input :access_token %>
  <%= f.submit 'Submit', id: 'submit_apartments' %>
<% end %>