<%= render partial: 'data' %>
<%= simple_form_for Apartment.new, multipart: true, remote: true do |f| %>
  <%= f.input :name %>
  <%= f.input :rate %>
  <%= f.input :address, id: 'address'%>
  <%= f.input :description  %>
  <%= f.input :access_token %>
  <%= f.submit 'Submit', id: 'submit_apartments' %>
<% end %>